{
  "name": "con4gis/tracking",
  "type": "contao-bundle",
  "description": "Tracking extension for con4gis-Maps",
  "keywords": ["con4gis", "tracking", "contao", "c4g", "maps", "gps"],
  "homepage": "https://con4gis.org",
  "license": "LGPL-3.0-or-later",
  "authors": [
    {
      "name": "Kuestenschmiede GmbH Software & Design",
      "homepage": "https://kuestenschmiede.de",
      "role": "Operator"
    },
    {
      "name": "con4gis community",
      "homepage": "https://con4gis.org",
      "role": "Developer"
    }
  ],
  "support": {
    "email": "support@con4gis.org",
    "issues": "https://github.com/Kuestenschmiede/TrackingBundle/issues"
  },
  "require": {
    "contao/core-bundle": "^4.4",
    "con4gis/maps": "^5.1",
    "vinyvicente/doctrine-point-type": "^1.1"
  },
  "conflict": {
    "contao/core": "*",
    "contao/manager-plugin": "<2.0 || >=3.0"
  },
  "autoload": {
    "psr-4": {
      "con4gis\\TrackingBundle\\": ""
    },
    "classmap": [
      "Resources/contao/"
    ],
    "exclude-from-classmap": [
      "Resources/contao/config/",
      "Resources/contao/dca/",
      "Resources/contao/languages/",
      "Resources/contao/templates/"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "contao-manager-plugin": "con4gis\\TrackingBundle\\ContaoManager\\Plugin"
  }
}
